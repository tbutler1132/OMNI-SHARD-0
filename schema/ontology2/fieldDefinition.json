{
  "entity": "FieldDefinition",
  "version": 1,
  "description": "Defines the semantic structure, validations, governance, and UI hints for a single field in the Ontology.",
  "fields": [
    {
      "id": "field-fielddefinition-id",
      "name": "id",
      "type": "string",
      "optional": false,
      "requires_human": true,
      "input_type": "text",
      "description": "Unique identifier for this FieldDefinition.",
      "prompt": "What is the ID of the FieldDefinition?",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-name",
      "name": "name",
      "type": "string",
      "optional": false,
      "requires_human": true,
      "input_type": "text",
      "description": "Short name of the field (e.g., 'title', 'status').",
      "prompt": "What is the name of the field?",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-type",
      "name": "type",
      "type": "string",
      "optional": false,
      "requires_human": true,
      "input_type": "select",
      "enum_values": [
        "string",
        "number",
        "boolean",
        "datetime",
        "enum",
        "reference",
        "reference[]",
        "object",
        "object[]",
        "string[]"
      ],
      "description": "The type of data this field holds.",
      "prompt": "Select the type of this field.",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-optional",
      "name": "optional",
      "type": "boolean",
      "optional": true,
      "requires_human": true,
      "input_type": "checkbox",
      "description": "Is this field optional?",
      "prompt": "Is this field optional?",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-requiresHuman",
      "name": "requires_human",
      "type": "boolean",
      "optional": true,
      "requires_human": true,
      "input_type": "checkbox",
      "description": "Does a human need to provide this field value?",
      "prompt": "Should a human provide this field value?",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-autoPopulated",
      "name": "auto_populated",
      "type": "boolean",
      "optional": true,
      "requires_human": true,
      "input_type": "checkbox",
      "description": "Is this field automatically generated by the system?",
      "prompt": "Is this field auto-populated?",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-inputType",
      "name": "input_type",
      "type": "string",
      "optional": true,
      "requires_human": true,
      "input_type": "select",
      "enum_values": [
        "text",
        "number",
        "datetime",
        "select",
        "checkbox",
        "json-editor",
        "reference-select",
        "reference-multi-select"
      ],
      "description": "UI hint for rendering input control.",
      "prompt": "Select the input type for the field.",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-outputType",
      "name": "output_type",
      "type": "string",
      "optional": true,
      "requires_human": true,
      "input_type": "text",
      "description": "Optional output display type hint.",
      "prompt": "Specify output display format (optional).",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-maxLength",
      "name": "max_length",
      "type": "number",
      "optional": true,
      "requires_human": true,
      "input_type": "number",
      "description": "Maximum allowed length (for string fields).",
      "prompt": "Enter max character length (optional).",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-enumValues",
      "name": "enum_values",
      "type": "string[]",
      "optional": true,
      "requires_human": true,
      "input_type": "json-editor",
      "description": "Allowed enum options for enum fields.",
      "prompt": "Enter allowed values if type is enum (optional).",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-determinationRules",
      "name": "determination_rules",
      "type": "object",
      "optional": true,
      "requires_human": true,
      "input_type": "json-editor",
      "description": "Dynamic rules for how this field is populated.",
      "prompt": "Specify any dynamic determination rules (optional).",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-governedBy",
      "name": "governed_by",
      "type": "reference",
      "target": "Protocol",
      "optional": true,
      "requires_human": true,
      "input_type": "reference-select",
      "description": "Protocol that governs this field (optional).",
      "prompt": "Link a Protocol that governs this field (optional).",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-description",
      "name": "description",
      "type": "string",
      "optional": true,
      "requires_human": true,
      "input_type": "text",
      "description": "Human-readable description of this field.",
      "prompt": "Write a description for this field (optional).",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-prompt",
      "name": "prompt",
      "type": "string",
      "optional": true,
      "requires_human": true,
      "input_type": "text",
      "description": "Prompt text for forms.",
      "prompt": "What should the user be asked for this field?",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-schemaVersion",
      "name": "schema_version",
      "type": "number",
      "optional": false,
      "requires_human": false,
      "auto_populated": true,
      "input_type": "number",
      "description": "Version number of the FieldDefinition model.",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-createdAt",
      "name": "created_at",
      "type": "datetime",
      "optional": true,
      "requires_human": false,
      "auto_populated": true,
      "input_type": "datetime",
      "description": "Timestamp when the FieldDefinition was created.",
      "schema_version": 1
    },
    {
      "id": "field-fielddefinition-updatedAt",
      "name": "updated_at",
      "type": "datetime",
      "optional": true,
      "requires_human": false,
      "auto_populated": true,
      "input_type": "datetime",
      "description": "Timestamp when the FieldDefinition was last updated.",
      "schema_version": 1
    }
  ]
}
